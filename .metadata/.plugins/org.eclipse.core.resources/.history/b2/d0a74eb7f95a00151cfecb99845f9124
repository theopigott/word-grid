
public class WordGrid {
	static int n = 3;
	static int m = 3;
	static Square[][] squares;
	
	public static void main(String[] args) {
		RandomLetter rl = new RandomLetter();
		squares = new Square[n][m];
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < m; j++) {
				squares[i][j] = new Square(i, j, )
			}
		}

	}

}
